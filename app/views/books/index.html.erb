<%= form_for @my_search do |f| %>
  <%= f.slide_selector [:min_price, :max_price], [0, 500], amount_drop_down_options, type: 'amount', selector: false, slider_step: 5 %>
  <br><br>
  <%= f.slide_selector [:min_rank, :max_rank], [0, 5000], simple_drop_down_options %>
  <br><br>
  <%= f.submit :submit %>
<% end %>
